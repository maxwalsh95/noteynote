{
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error",
    "no-unused-vars": "warn",
    "func-names": "off",
    "class-methods-use-this": "off",
    "no-sequences": "off",
    "no-param-reassign": "off",
    "no-plusplus": "off",
    "no-underscore-dangle": "off",
    "import/extensions": "off",
    "no-use-before-define": "warn"
  },
  "env": {
    "browser": true
  }
}
